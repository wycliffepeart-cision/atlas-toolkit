{
  "name": "@atlas/toolkit",
  "version": "1.0.0",
  "scripts": {
    "test": "vitest --dom",
    "test:ui": "vitest --dom --ui",
    "test:coverage": "vitest run --dom --coverage",
    "test:run": "vitest run",
    "build": "npx rollup -c && npm run generate:types",
    "prettier:fix": "prettier --write \"./**/*.{html,ts,tsx,css,js,md,json}\"",
    "prettier:check": "prettier --check \"./**/*.{html,ts,tsx,css,js,md,json}\" --config ./.prettierrc",
    "generate:types": "npx -p typescript tsc -rootDir src --declaration --allowJs --emitDeclarationOnly --outDir dist --declarationMap",
    "sb:start": "cd stories && npm run storybook",
    "sb:run": "cd stories && npm i && npm run storybook",
    "build-storybook": "build-storybook",
    "cy:open": "cypress open",
    "cy:run": "cypress run --component",
    "storybook": "start-storybook -p 6006"
  },
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/html": "^6.1.21",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^6.1.21",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^29.4.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vitest/ui": "^0.28.5",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.7.3",
    "cypress": "^12.5.1",
    "file-loader": "^6.2.0",
    "happy-dom": "^8.4.3",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-multi-input": "^1.3.3",
    "rollup-plugin-posthtml-template": "^1.3.0",
    "rollup-plugin-svg": "^2.0.0",
    "storybook-dark-mode": "^2.0.6",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  },
  "dependencies": {
    "fuse.js": "^6.6.2"
  }
}
